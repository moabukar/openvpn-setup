# OpenVPN Server Configuration
# Copy to .env and customise

# Server settings
SERVER_IP=your.server.ip
VPN_PORT=1194
VPN_PROTOCOL=udp

# Network settings
VPN_SUBNET=10.8.0.0
VPN_NETMASK=255.255.255.0

# Security settings
CIPHER=AES-256-GCM
AUTH=SHA256
TLS_VERSION_MIN=1.2

# Paths
OPENVPN_DIR=/etc/openvpn
LOG_DIR=/var/log/openvpn
CLIENT_CONFIG_DIR=/etc/openvpn/client-configs
